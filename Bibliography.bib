@inproceedings{gtitm,
abstract = {Graphs are commonly used to model the structure of internetworks,$\backslash$nfor the study of problems ranging from routing to resource reservation.$\backslash$nA variety of graph models are found in the literature, including regular$\backslash$ntopologies such as rings or stars, {\&}ldquo;well-known{\&}rdquo; topologies$\backslash$nsuch as the original ARPAnet, and randomly generated topologies. Less$\backslash$ncommon is any discussion of how closely these models correlate with real$\backslash$nnetwork topologies. We consider the problem of efficiently generating$\backslash$ngraph models that accurately reflect the topological properties of real$\backslash$ninternetworks. We compare the properties of graphs generated using$\backslash$nvarious methods with those of real internets. We also propose efficient$\backslash$nmethods for generating topologies with particular properties, including$\backslash$na transit-stub model that correlates well with the internet structure.$\backslash$nImproved models for the internetwork structure have the potential to$\backslash$nimpact the significance of simulation studies of internetworking$\backslash$nsolutions, providing a basis for the validity of the conclusions},
address = {Washington, DC, USA},
author = {Zegura, E.W. and Calvert, K.L. and Bhattacharjee, S.},
